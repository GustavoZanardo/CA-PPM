			  END))
	 ELSE
		 (sum(CASE WHEN p.odf_object_code = 'other' THEN nvl(s_alloc.slice,0) 
						WHEN (p.odf_object_code =  'project' or p.odf_object_code = 'idea') THEN nvl(s_est.slice,0)+nvl(s_act.slice,0) END) /
